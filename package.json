{
  "name": "hakion-ui",
  "version": "0.0.1",
  "description": "hakion-ui component library",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/hakion-ui/hakion-ui.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/hakkei-co/hakion-ui.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "lint": "eslint src/**/*{.ts,.tsx}",
    "prettier": "npm run prettier:base -- --write",
    "prettier:base": "prettier \"./{src,test}/**/*.{ts,tsx,js,jsx}\"",
    "prettier:dry-run": "npm run prettier:base -- --list-different"
  },
  "dependencies": {
    "@ionic/core": "^6.0.9",
    "@stencil/core": "^2.13.0",
    "@stencil/router": "^1.0.1",
    "dark-mode-toggle": "^0.10.0"
  },
  "devDependencies": {
    "@stencil/eslint-plugin": "^0.4.0",
    "@stencil/postcss": "^2.1.0",
    "@stencil/sass": "^1.5.2",
    "@stencil/store": "^1.5.0",
    "@types/autoprefixer": "^10.2.0",
    "@types/jest": "^27.0.3",
    "autoprefixer": "^10.4.2",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "prettier": "2.5.1",
    "prettier-eslint": "^13.0.0",
    "puppeteer": "^10.0.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "three": "^0.138.0",
    "three-elements": "^0.3.0"
  },
  "author": "jlau <jonathan@torr.email>",
  "license": "UNLICENSED",
  "publishConfig": {
    "@hakkei-co:registry": "https://npm.pkg.github.com"
  }
