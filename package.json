{
  "name": "@phase2/outline",
  "description": "Outline Component Design",
  "main": "dist/outline.js",
  "module": "dist/outline.js",
  "es2015": "dist/outline-legacy.js",
  "es2017": "dist/outline.js",
  "unpkg": "dist/outline.js",
  "files": [
    "dist/",
    ".prettierrc.json",
    ".eslintrc.json",
    "tailwind.config.js",
    "outline.theme.css"
  ],
  "author": {
    "name": "Phase2 Technology",
    "url": "https://phase2technology.com"
  },
  "license": "BSD-3-Clause",
  "publishConfig": {
    "registry": "https://us-central1-npm.pkg.dev/phase2-devcloud/phase2-npm/"
  },
  "sbport": 6042,
  "scripts": {
    "prestart": "yarn prep",
    "start": "yarn storybook.dev.watch",
    "prep": "yarn generate.safelist && yarn generate.custom-elements && yarn generate.component-vars && yarn generate.resolved-tailwind && yarn generate.resolved-outline && yarn generate.component-list && yarn generate.css && yarn generate.assets",
    "generate.resolved-tailwind": "node scripts/resolve-tailwind-config.js",
    "generate.resolved-outline": "node scripts/resolve-outline-config.js",
    "generate.safelist": "node scripts/generate-tw-safelist.js",
    "generate.custom-elements": "wca analyze 'src/components/**/!(*.stories|*.lit|*.test).ts' --format json --outFile ./src/custom-elements.json",
    "generate.component-list": "wca analyze 'src/components/**/!(*.stories|*.lit|*.test).ts' --features --format json --outFile ./src/component-list.json",
    "generate.component-vars": "node scripts/generate-loader-config.js",
    "generate.assets": "node scripts/bundle-assets.js",
    "generate.css": "node scripts/styles.js",
    "generate.icons": "yarn run fmt.svg && node ./scripts/generate-icons.js ",
    "prebuild": "yarn prep",
    "build": "yarn tsc && yarn rollup",
    "build.watch": "concurrently -c \"green,yellow\" \"yarn:tsc --watch\" \"yarn:rollup.watch\" --kill-others",
    "wds": "wds --app-index dev/index.html --open dev/ --watch --node-resolve",
    "preserve": "yarn prep",
    "serve": "concurrently -c \"cyan,blue\" \"yarn:wds\" \"yarn:build.watch\" --kill-others",
    "storybook.axe.prod": "yarn storybook.prod && yarn axe-storybook --build-dir=storybook-static/",
    "storybook.axe.dev": "yarn storybook.axe.dev.chromium && yarn storybook.axe.dev.firefox",
    "storybook.axe.dev.chromium": "yarn axe-storybook --storybook-address=\"http://localhost:${npm_package_sbport}\" --browser chromium",
    "storybook.axe.dev.firefox": "yarn axe-storybook --storybook-address=\"http://localhost:${npm_package_sbport}\" --browser firefox",
    "storybook.axe.dev.webkit": "yarn axe-storybook --storybook-address=\"http://localhost:${npm_package_sbport}\" --browser webkit",
    "storybook.wait": "wait-on ./dist/storybook.css ./src/custom-elements.json ./src/resolved-tailwind-config.ts",
    "storybook.dev": "start-storybook -c src/.storybook -p ${npm_package_sbport}",
    "storybook.dev.watch": "concurrently -n \"css,storybook\" -c \"green,blue\" \"yarn:generate.css --watch\" \"yarn:storybook.dev\" --kill-others",
    "storybook.prod": "yarn prep && yarn storybook.wait && NODE_ENV=production build-storybook -c src/.storybook -o storybook-static",
    "lint": "yarn lint.eslint && yarn lint.tsc && yarn lint.prettier",
    "lint.tsc": "yarn tsc --noEmit",
    "lint.eslint": "eslint .",
    "lint.prettier": "prettier --check .",
    "fmt": "concurrently \"yarn:fmt.code\" \"yarn:fmt.svg\"",
    "fmt.code": "eslint --fix . && prettier --write .",
    "fmt.svg": "svgo -f ./src -r --config=svgo.config.js",
    "fix": "yarn fmt && yarn lint",
    "ci": "rm -rf node_modules && yarn install --frozen-lockfile",
    "clean": "rimraf dist && rimraf src/custom-elements.json && rimraf src/resolved-tailwind-config.ts && rimraf storybook-static && rimraf src/**/*.lit.ts",
    "reset": "rimraf node_modules",
    "reboot": "yarn clean && yarn reset && yarn install && yarn start",
    "rollup.bundle": "rollup -c -m",
    "rollup.watch": "yarn rollup -w",
    "rollup": "yarn rollup.bundle",
    "test": "yarn build && wtr"
  },
  "keywords": [
    "web-components",
    "typescript",
    "lit"
  ],
  "dependencies": {
    "@splidejs/splide": "^3.2.1",
    "lit": "^2.0.0"
  },
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.14.2",
    "@babel/preset-flow": "^7.16.7",
    "@chanzuckerberg/axe-storybook-testing": "^5.0.0",
    "@commitlint/cli": "^13.2.0",
    "@commitlint/config-conventional": "^13.2.0",
    "@open-wc/testing": "^3.0.0-next.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-multi-entry": "^4.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@storybook/addon-a11y": "^6.4.19",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/web-components": "^6.4.19",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "@web/dev-server": "0.0.29",
    "@web/dev-server-legacy": "^0.1.4",
    "@web/test-runner": "^0.13.15",
    "@web/test-runner-browserstack": "^0.4.2",
    "@web/test-runner-playwright": "^0.8.6",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-named-exports-order": "^0.0.2",
    "concurrently": "^6.3.0",
    "deepmerge": "^4.2.2",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.23.0",
    "gaze": "^1.1.3",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "lint-staged": "^10.5.4",
    "postcss": "^8.4.4",
    "postcss-cli": "^8.3.1",
    "postcss-custom-properties": "^11.0.0",
    "postcss-discard-comments": "^5.1.0",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^5.2.0",
    "postcss-preset-env": "^7.0.0",
    "prettier": "^2.0.4",
    "prettier-plugin-tailwindcss": "^0.1.7",
    "rimraf": "^3.0.2",
    "rollup": "^2.46.0",
    "rollup-plugin-summary": "^1.3.0",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "rsync": "^0.6.1",
    "storybook": "^6.4.19",
    "svgo": "^2.7",
    "tailwindcss": "^3.0.0",
    "ts-lit-plugin": "^1.2.1",
    "typescript": "^4.0.0",
    "wait-on": "^5.3.0",
    "web-component-analyzer": "^1.0.3",
    "webpack": "^5.38.1"
  }
}
